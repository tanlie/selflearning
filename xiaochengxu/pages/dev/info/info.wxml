<import src="../../../lib/template/tool.wxml" />
<view class="page__bd">
  <view class='reg-box-group'>
    <view class="weui-cells weui-cells_after-title reg-box">
      <view class="weui-cell">
          <view class="weui-cell__bd"><text class='f-red'>* </text>设备唯一码</view>
          <view class="weui-cell__ft">83499</view>
      </view>
      <view class="weui-cell">
          <view class="weui-cell__bd"><text class='f-red'>* </text>设备类型</view>
          <view class="weui-cell__ft">街霸</view>
      </view>
      <navigator url="../upd/upd" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
          <view class="weui-cell__bd"><text class='f-red'>* </text>投放地址</view>
          <view class="weui-cell__ft weui-cell__ft_in-access">美宜佳</view>
      </navigator>
      <navigator url="../upd/upd" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
          <view class="weui-cell__bd"><text class='f-red'>* </text>设备名称</view>
          <view class="weui-cell__ft weui-cell__ft_in-access">1号街机</view>
      </navigator>
      <navigator url="../upd/upd" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
          <view class="weui-cell__bd"><text class='f-red'>* </text>价格</view>
          <view class="weui-cell__ft weui-cell__ft_in-access">2元/次</view>
      </navigator>
      <view class="weui-cell weui-cell_switch">
          <view class="weui-cell__bd">禁用设备</view>
          <view class="weui-cell__ft">
              <switch />
          </view>
      </view>
      <view class="weui-cell">
        <view class="weui-cell__bd"></view>
        <view class="weui-cell__ft"></view>
      </view>
      <button class="weui-btn" type="default">解绑设备</button>
    </view>
  </view>
</view>